pipeline{
	agent any

	stages{

		stage('build jar'){
			steps{
				 bat "mvn clean package -DskipTests"
			}
		}

		stage('build image'){
			steps{
				 bat "docker build -t=framework_with_jenkins ."
			}
		}

		stage('push image'){
            steps{
                 bat "docker push dimbas/getting-started:version_1 ."
            }
        }

	}

}